							"deleting": false
						}
					}
				},
				"source": [
					"from pyspark.sql.functions import col\r\n",
					"for clmn in df_all.columns:\r\n",
					"    nullcount = df_all.filter(col(clmn).isNull()).count() \r\n",
					"    if(nullcount>0):\r\n",
					"        print('Null count is ' + clmn +str (nullcount))"
				],
				"execution_count": 7
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"df_clean = df_all.fillna('unknown')\r\n",
					"for clmn in df_clean.columns:\r\n",
					"    nullcount = df_clean.filter(col(clmn).isNull()).count() \r\n",
					"    if(nullcount>0):\r\n",
