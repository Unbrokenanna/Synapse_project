						}
					}
				},
				"source": [
					"df_all = df_nodups.dropna(how='all')\r\n",
					"print(df_all.count())"
				],
				"execution_count": 4
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
